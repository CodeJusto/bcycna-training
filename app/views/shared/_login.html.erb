<h3>Welcome back.</h3>
<%= form_tag sessions_path do |f| %>
  <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <%= text_field_tag :email %>
          <label for="icon_prefix">Email</label>
        </div>
        <div class="input-field col s6">
          <%= password_field_tag :password %>
          <label for="icon_telephone">Password</label>
        </div>
      </div>
      <div class="modal-footer">
        <div class="row">
          <%= submit_tag "Submit" %>
        </div>
      </div>
    </form>
  </div>
<% end %>